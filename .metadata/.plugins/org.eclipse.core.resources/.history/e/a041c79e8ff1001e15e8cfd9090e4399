package day2;

public class DataTypeDemo {

	public static void main(String[] args) {

		// premitive data type byte,shot,int,long ,flot ,double,char,boolean

		int a; // delcaration
		a = 100; // Assignement

		int b = 100; // declaration and Assignement
		System.out.println(a + b);
			
		int ab,bc,de;
		ab= 33;
		bc=32;
		de = 45;
		System.out.println(ab+bc+de);
		
		// non premitive data type /collection / derived data type
		// String,ArrayList,Hashset,

	}

}
