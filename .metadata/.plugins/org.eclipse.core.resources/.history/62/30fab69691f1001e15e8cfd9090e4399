package day2;

public class DataTypeDemo2 {

	public static void main(String[] args) {

		int a=100, b=200;
		System.out.println( "this value of a is :"+a);
		System.out.println("this value of b is :"+b);
		
		long x= 1234341234;
	System.out.println("this value of x is :" +x);		
		byte by = 100 ;
		
		System.out.println("this value of by is :" +by);
      short sh = 1345 ;
		
		System.out.println("this value of sh is :" +sh);
		
		//decimal number -float ,double 
		float item_price = 15.50f;
		System.out.println("this value of item_price  is :"+item_price);
		
		
	}

}
